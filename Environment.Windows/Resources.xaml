<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Environment.Windows">

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <local:MultivalueErrorConverter x:Key="MultivalueError" />
    <local:MultivalueBoolInvertConverter x:Key="MultivalueBoolInvert" />
    <local:MultivalueTextBoxesConverter x:Key="MultivalueTextBoxes" />
    <local:MultivalueBoolInvertConverter x:Key="NameToColorConverter" />
    <local:InvertBooleanConverter x:Key="InvertBooleanConverter" />

    <Style x:Key="SButton" TargetType="Button">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="#d0cece"/>
        <Setter Property="Height" Value="25"/>
    </Style>

</ResourceDictionary>